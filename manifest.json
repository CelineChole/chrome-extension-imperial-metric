{
  "name": "A world in metric",
  "version": "1.0",
  "description": "Convert US cups metric measures.",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "jquery-3.3.1.min.js",
      "toastr.min.js",
      "content.js"
    ],
    "css": [
      "toastr.min.css"
    ]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_icon": "Cooking-icon.png"
  },
  "manifest_version": 2
}


