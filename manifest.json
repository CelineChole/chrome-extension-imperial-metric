{
  "name": "A world im metric",
  "version": "1.0",
  "description": "Convert US cups metric measures.",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "Cooking-icon.png",
    "default_popup": "popup.html",
    "default_tittle": "A popup will come here."
  },
  "manifest_version": 2
}


